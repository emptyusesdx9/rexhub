})

-- sections

AutoTab:AddButton({
	Name = "Auto Click",
	Callback = function()
        while wait(0.1) do
            game:GetService("ReplicatedStorage"):WaitForChild("Assets"):WaitForChild("Events"):WaitForChild("TapClick"):WaitForChild("RemoteEvent"):FireServer()
            end
  	end    
})

