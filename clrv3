QuestTab:AddButton({
	Name = "Teleport to Quest NPC",
	Callback = function()
        local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
        rootPart.CFrame = CFrame.new(-785.140259, 882.183105, -778.013977, -0.499959469, 0, 0.866048813, 0, 1, 0, -0.866048813, 0, -0.499959469)
  	end    
})

QuestTab:AddButton({
	Name = "Auto Get Gear",
	Callback = function()
        local function teleportAndClick()
            -- Acessar diretamente a parte no Workspace
            local part = game:GetService("Workspace"):FindFirstChild("Riddles"):FindFirstChild("Gears"):FindFirstChild("Piece")
            if part and part.ClassName == "MeshPart" then
                -- Teleportar o jogador para a posi√ß√£o da pe√ßa
                local player = game.Players.LocalPlayer
                if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                    player.Character.HumanoidRootPart.CFrame = part.CFrame
                    
                    -- Esperar um pequeno intervalo para garantir que a teletransporta√ß√£o foi conclu√≠da
                    wait(0.3)
                    
                    -- Mudar o MaxActivationDistance do ClickDetector e clicar
                    local clickDetector = part:FindFirstChild("ClickDetector")
                    if clickDetector then
                        clickDetector.MaxActivationDistance = 10000
                        fireclickdetector(clickDetector)
                        print("Success")
                        
                        wait(0.7)
                        local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
                        rootPart.CFrame = CFrame.new(-785.140259, 882.183105, -778.013977, -0.499959469, 0, 0.866048813, 0, 1, 0, -0.866048813, 0, -0.499959469)
                    end
                end
            end
        end
        
        teleportAndClick()
    end
})


OrionLib:Init()
