local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = " (250K) Clicker Legends Final v1.5 ÔΩú by rex", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--main

