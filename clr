local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Clicker Legends Reborn ÔΩú by rex", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

--main

local AutoTab = Window:MakeTab({
	Name = "üîßAuto Farm!",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local EggsTab = Window:MakeTab({
	Name = "Eggs",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local QuestTab = Window:MakeTab({
	Name = "DJ Boom Quest",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MiscTab = Window:MakeTab({
	Name = "ü™ùMisc",
