EggsTab:AddButton({
	Name = "Common 3x Spam",
	Callback = function()
        while wait(0.3) do
            local args = {
            [1] = "Common Egg",
            [2] = 3,
            [3] = true
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("Assets"):WaitForChild("Events"):WaitForChild("PurchaseEgg"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
        end
  	end    
})

