local Button = PlayerTab:CreateButton({
   Name = "Auto Claim All Achievements",
   Callback = function()
      while wait(0) do
         local args = {
            [1] = "ClaimAchievement",
            [2] = 1
        }
         game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
local args = {
            [1] = "ClaimAchievement",
            [2] = 2
            }
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
        local args = {
            [1] = "ClaimAchievement",
            [2] = 3
            }
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):FireServer(unpack(args))
        local args = {
            [1] = "ClaimAchievement",
            [2] = 4
