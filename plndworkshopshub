local Button = DupeTab:CreateButton({
   Name = "Step 1 (click)",
   Callback = function()
      function Death()
         spawn(function()
 local args = {
     [1] = "GetPlayerData"
 }
 
 game:GetService("ReplicatedStorage").NetworkRemoteFunction:InvokeServer(unpack(args))
 end)
 end
   end,
})

local Button = DupeTab:CreateButton({
   Name = "Step 2 (Click)",
   Callback = function()
      function Death()
         spawn(function()
 local args = {
     [1] = "DefaultData"
 }
 
 game:GetService("ReplicatedStorage").NetworkRemoteFunction:InvokeServer(unpack(args))
 end)
 end
   end,
})

